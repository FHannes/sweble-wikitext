/*!
 *
 * Compiler Log
 * ------------
 *
 *   AST node:
 *     Name        : CompilerLog
 *     Extends     : ContentNode
 *
 *     Properties:
 *       title    : String
 *       revision : Long
 *
 * Validator Log
 * -------------
 *
 *   AST node:
 *     Name        : ValidatorLog
 *     Extends     : ContentNode
 *     
 *     Properties:
 *       timeNeeded : Long
 *
 * Preprocessor Log
 * ----------------
 *
 *   AST node:
 *     Name        : PreprocessorLog
 *     Extends     : ContentNode
 *     
 *     Properties:
 *       forInclusion : boolean
 *       timeNeeded   : Long
 *
 * Preprocessor Resolver Log
 * -------------------------
 *
 *   AST node:
 *     Name        : PpResolverLog
 *     Extends     : ContentNode
 *     
 *     Properties:
 *       timeNeeded : Long
 *
 * Parser Log
 * ----------
 *
 *   AST node:
 *     Name        : ParserLog
 *     Extends     : ContentNode
 *     
 *     Properties:
 *       timeNeeded : Long
 *
 * Postprocessor Log
 * -----------------
 *
 *   AST node:
 *     Name        : PostprocessorLog
 *     Extends     : ContentNode
 *     
 *     Properties:
 *       timeNeeded : Long
 *
 * Resolve Redirect Log
 * --------------------
 *
 *   AST node:
 *     Name        : ResolveRedirectLog
 *     Extends     : ContentNode
 *     Constructor : "target, success"
 *     
 *     Properties:
 *       success    : boolean
 *       target     : String
 *       canonical  : String
 *       timeNeeded : Long
 *
 * Resolve Transclusion Log
 * ------------------------
 *
 *   AST node:
 *     Name        : ResolveTransclusionLog
 *     Extends     : ContentNode
 *     Constructor : "target, success"
 *     
 *     Properties:
 *       success    : boolean
 *       target     : String
 *       canonical  : String
 *       timeNeeded : Long
 *
 * Parse Exception
 * ---------------
 *
 *   AST node:
 *     Name        : ParseException
 *     Extends     : LeafNode
 *     Constructor : message
 *     
 *     Properties:
 *       message : String
 *
 * Illegal Name Exception
 * ----------------------
 *
 *   AST node:
 *     Name        : IllegalNameException
 *     Extends     : InnerNode.InnerNode1
 *     Constructor : "name, message"
 *
 *     Properties:
 *       message    : String
 *
 *     Children:
 *       name : AstNode
 *
 * Unhandled Exception
 * -------------------
 *
 *   AST node:
 *     Name        : UnhandledException
 *     Extends     : LeafNode
 *     Constructor : "exception, dump"
 *     
 *     Properties:
 *       exception : Throwable
 *       dump      : String
 *
 */

module org.sweble.wikitext.engine.log.LogNodes;
