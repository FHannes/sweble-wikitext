WtParsedWikitextPage(
  Properties:
    {N} entityMap = EntityMap {}
    {N} warnings = []

  [
    Table(
      Properties:
        {N} rtd = RTD[ "{|" <o> "\n" <o> " |}" ]

      [
        WtXmlAttribute(
          Properties:
            {N} hasValue = true
            {N} name = "border"
            {N} rtd = RTD[ " border=\"" <o> "\"" ]

          [ WtText("1") ]
        )
      ]
      [
        TableHeader(
          Properties:
            {N} rtd = RTD[ " !" <o> " |" <o> "" ]

          [
            WtXmlAttribute(
              Properties:
                {N} hasValue = true
                {N} name = "class"
                {N} rtd = RTD[ " class=\"" <o> "\"" ]

              [ WtText("bar") ]
            )
          ]
          [
            WtParagraph([ WtText(" Header") ])
            WtNewline("\n")
          ]
        )
      ]
    )
    WtNewline("\n")
    WtNewline("\n")
    Table(
      Properties:
        {N} rtd = RTD[ "{|" <o> "\n" <o> " |}" ]

      [ ]
      [
        TableRow(
          Properties:
            {N} rtd = RTD[ " |-" <o> "\n" <o> "" ]

          [
            WtXmlAttribute(
              Properties:
                {N} hasValue = true
                {N} name = "border"
                {N} rtd = RTD[ " border=\"" <o> "\"" ]

              [ WtText("2") ]
            )
          ]
          [
            TableHeader(
              Properties:
                {N} rtd = RTD[ " !" <o> "" <o> "" ]

              [ ]
              [
                WtParagraph([ WtText(" class=\"foo\"") ])
                WtNewline("\n")
              ]
            )
          ]
        )
      ]
    )
  ]
)
