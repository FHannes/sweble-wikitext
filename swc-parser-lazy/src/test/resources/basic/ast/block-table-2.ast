WtParsedWikitextPage(
  Properties:
    {N} entityMap = EntityMap {}
    {N} warnings = []

  [
    WtTable(
      Properties:
        {N} rtd = RTD[ "{|" <o> "\n" <o> " |}" ]

      WtXmlAttributes([
        WtXmlAttribute(
          Properties:
            {N} name = "border"
            {N} rtd = RTD[ " border=\"" <o> "\"" ]

          WtValue([ WtText("1") ])
        )
      ])
      WtBody([
        WtTableHeader(
          Properties:
            {N} rtd = RTD[ " !" <o> " |" <o> "" ]

          WtXmlAttributes([
            WtXmlAttribute(
              Properties:
                {N} name = "class"
                {N} rtd = RTD[ " class=\"" <o> "\"" ]

              WtValue([ WtText("bar") ])
            )
          ])
          WtBody([
            WtParagraph([ WtText(" Header") ])
            WtNewline("\n")
          ])
        )
      ])
    )
    WtNewline("\n")
    WtNewline("\n")
    WtTable(
      Properties:
        {N} rtd = RTD[ "{|" <o> "\n" <o> " |}" ]

      -
      WtBody([
        WtTableRow(
          Properties:
            {N} rtd = RTD[ " |-" <o> "\n" <o> "" ]

          WtXmlAttributes([
            WtXmlAttribute(
              Properties:
                {N} name = "border"
                {N} rtd = RTD[ " border=\"" <o> "\"" ]

              WtValue([ WtText("2") ])
            )
          ])
          WtBody([
            WtTableHeader(
              Properties:
                {N} rtd = RTD[ " !" <o> "" <o> "" ]

              -
              WtBody([
                WtParagraph([ WtText(" class=\"foo\"") ])
                WtNewline("\n")
              ])
            )
          ])
        )
      ])
    )
  ]
)
