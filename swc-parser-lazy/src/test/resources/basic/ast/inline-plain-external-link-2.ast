WtParsedWikitextPage(
  Properties:
    {N} entityMap = EntityMap {}
    {N} warnings = []

  [
    HorizontalRule(
      Properties:
        {N} rtd = RTD[ "----" ]
    )
    WtParagraph([
      Url(
        Properties:
          {N} path = "//example.com"
          {N} protocol = "http"
          {N} rtd = RTD[ "http://example.com" ]
      )
    ])
    Newline("\n")
    HorizontalRule(
      Properties:
        {N} rtd = RTD[ "----" ]
    )
    Newline("\n")
    Newline("\n")
    WtParagraph([
      WtText("before ")
      Url(
        Properties:
          {N} path = "mustermann@musterfirma.com"
          {N} protocol = "mail"
          {N} rtd = RTD[ "mail:mustermann@musterfirma.com" ]
      )
      WtText(" after")
    ])
    Newline("\n")
  ]
)
