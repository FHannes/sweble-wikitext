WtParsedWikitextPage(
  Properties:
    {N} entityMap = EntityMap {0=WtXmlComment(" comment ... ")}
    {N} warnings = []

  [
    WtParagraph([ WtText("Hallo") ])
    WtNewline("\n")
    WtNewline("\n")
    WtParagraph([
      WtXmlComment(
        Properties:
          {N} content = " comment ... "
          {N} prefix = "  "
          {N} rtd = RTD[ "  <!-- comment ... -->  \n" ]
          {N} suffix = "  \n"
      )
      WtNewline("\n")
      WtText("Hallo")
    ])
  ]
)
