WtParsedWikitextPage(
  Properties:
    {N} entityMap = EntityMap {}
    {N} warnings = []

  [
    WtTable(
      Properties:
        {N} rtd = RTD[ "{|" <o> "\n" <o> " |}" ]

      -
      WtBody([
        WtTableCaption(
          Properties:
            {N} rtd = RTD[ " |+" <o> "" <o> "" ]

          -
          WtBody([
            WtParagraph([ WtText("x") ])
            WtNewline("\n")
          ])
        )
      ])
    )
  ]
)
