WtParsedWikitextPage(
  Properties:
    {N} entityMap = EntityMap {}
    {N} warnings = []

  [
    WtParagraph([
      WtItalics(
        Properties:
          {N} rtd = RTD[ "''" <o> "''" ]

        [
          WtText(" A ")
          WtBold(
            Properties:
              {N} rtd = RTD[ "'''" <o> "'''" ]

            [ WtText(" B ") ]
          )
          WtText(" C ")
        ]
      )
    ])
  ]
)
