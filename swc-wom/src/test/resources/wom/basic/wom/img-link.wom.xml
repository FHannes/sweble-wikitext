<?xml version="1.0" encoding="UTF-8"?>
<page title="Img-link" version="1.0" xmlns="http://sweble.org/schema/wom">
  <body>
    <p bottomgap="0" topgap="0">
      <image format="unrestrained" halign="default" source="Image:Test" valign="middle"/>
    </p>
    <p bottomgap="0" topgap="0">
      <image alt="Alt Text" format="unrestrained" halign="default"
        source="Image:Test" valign="middle"/>
    </p>
    <p bottomgap="0" topgap="0">
      <image alt="Alt Text" format="unrestrained" halign="default"
        source="Image:Test" valign="middle">
        <imgcaption>Title</imgcaption>
      </image>
    </p>
    <p bottomgap="0" topgap="0">
      <image format="unrestrained" halign="default" intlink=""
        source="Image:Test" valign="middle">
        <imgcaption>Title</imgcaption>
      </image>
    </p>
    <p bottomgap="0" topgap="0">
      <image extlink="http://example.com" format="unrestrained"
        halign="default" source="Image:Test" valign="middle">
        <imgcaption>Title</imgcaption>
      </image>
    </p>
    <p bottomgap="0" topgap="0">
      <image format="unrestrained" halign="default" intlink="Some Page"
        source="Image:Test" valign="middle">
        <imgcaption>Title</imgcaption>
      </image>
    </p>
    <p bottomgap="0" topgap="0">
      <image alt="Alt Text" border="border" format="unrestrained"
        halign="default" source="Image:Test" upright="upright" valign="middle">
        <imgcaption>Title</imgcaption>
      </image>
    </p>
    <image alt="Alt Text" border="border" format="frame" halign="right"
      source="Image:Test" upright="upright" valign="bottom">
      <imgcaption>Title</imgcaption>
    </image>
    <p bottomgap="0" topgap="0">
      <image alt="Alt Text" format="unrestrained" halign="default"
        source="Image:Test" valign="middle" width="128">
        <imgcaption>Title</imgcaption>
      </image>
    </p>
    <p bottomgap="0" topgap="0">
      <image alt="Alt Text" format="unrestrained" halign="default"
        height="512" source="Image:Test" valign="middle">
        <imgcaption>Title</imgcaption>
      </image>
    </p>
    <p bottomgap="0" topgap="0">
      <image alt="Alt Text" format="unrestrained" halign="default"
        height="512" source="Image:Test" valign="middle" width="128">
        <imgcaption>Title</imgcaption>
      </image>
    </p>
  </body>
</page>
